{
    "name": "surgiie/blade-cli",
    "description": "Use Laravel's blade engine via cli for rendering template files",
    "type": "template",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergio Compean",
            "email": "scompean24@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Surgiie\\BladeCLI\\": "src/"
        },
        "files":[
            "./src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Surgiie\\BladeCLI\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    },
    "require": {
        "php": "^8.0",
        "illuminate/console": "^9.0",
        "illuminate/view": "^9.0",
        "symfony/console": "^6.0",
        "vlucas/phpdotenv": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "symfony/var-dumper": "^6.0"
    }
}
